<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.nowon.mapper.MemberMapper"> 
<!--namespace = 클래스이름과 클래스의 위치 , id=추상메서드 메서드이름과 일치   -->
  <select id="findbyEmail" resultType="com.green.nowon.domain.dto.Member">
    select * from member where email =#{email}
  </select>
</mapper>